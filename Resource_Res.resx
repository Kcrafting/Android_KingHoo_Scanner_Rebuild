  <data name="ZZ_KIngHoo_LookUpInventory" xml:space="preserve">
    <value>CREATE PROCEDURE [dbo].[ZZ_KIngHoo_LookUpInventory]
@FNumber NVARCHAR(80)
AS
BEGIN
EXEC dbo.CheckInventory
SELECT C.FName [仓库/单据],D.FName 仓位,CAST(CAST(A.FQty AS DECIMAL(20,2)) AS NVARCHAR(20))+ E.FName 主计量,
CAST(CAST(A.FSecQty AS DECIMAL(20,2)) AS nvarchar(20)) +F.FName 辅助计量,A.FBatchNo 批号
  FROM dbo.ICInventory A 
JOIN dbo.t_ICItem B ON A.FItemID=B.FItemID 
JOIN dbo.t_Stock C ON A.FStockID=C.FItemID
LEFT JOIN dbo.t_StockPlace D ON A.FStockPlaceID=D.FSPID
LEFT JOIN dbo.t_MeasureUnit E ON B.FUnitID=E.FItemID
LEFT JOIN dbo.t_MeasureUnit F ON B.FSecUnitID=F.FItemID
WHERE B.FNumber = @FNumber
UNION
--预计出库量
--预计出库 条件：任务单未完结 减去已下推的领料数量
SELECT '投料单' [仓库/单据],'' 仓位,CAST(CAST((B.FQtyMust - B.FQty) AS DECIMAL(20,2)) AS NVARCHAR(20))+E.FName 主计量,'' 辅助计量,B.FBatchNo 批号
FROM dbo.PPBOM A 
JOIN dbo.PPBOMEntry B ON A.FInterID=B.FInterID 
JOIN dbo.ICMO C ON A.FICMOInterID=C.FInterID 
JOIN dbo.t_ICItem D ON B.FItemID = D.FItemID
JOIN dbo.t_MeasureUnit E ON B.FUnitID=E.FItemID
WHERE C.FStatus!=3 AND D.FNumber=@FNumber
UNION
SELECT '销售订单' [仓库/单据],'' 仓位,CAST(CAST((B.FQty-B.FCommitQty) AS DECIMAL(20,2)) AS NVARCHAR(20)) + D.FName 主计量,
CAST(CAST((B.FSecQty-B.FSecCommitQty) AS DECIMAL(20,2)) AS NVARCHAR(20)) 辅助计量,B.FBatchNo  批号
FROM dbo.SEOrder A JOIN dbo.SEOrderEntry B ON A.FInterID=B.FInterID
JOIN dbo.t_ICItem C ON B.FItemID=C.FItemID
JOIN dbo.t_MeasureUnit D ON B.FUnitID=D.FItemID
LEFT JOIN dbo.t_MeasureUnit E ON C.FSecUnitID=E.FItemID
WHERE A.FClosed!=1 AND C.FNumber=@FNumber
UNION
--预计入库量
SELECT '采购订单' [仓库/单据],'' 仓位, CAST(CAST((B.FQty-B.FCommitQty) AS DECIMAL(20,2)) AS NVARCHAR(20)) +  D.FName 主计量,
'' 辅助计量,'' 批号
FROM dbo.POOrder A JOIN dbo.POOrderEntry B ON A.FInterID=B.FInterID 
JOIN dbo.t_ICItem C ON B.FItemID=C.FItemID 
JOIN dbo.t_MeasureUnit D ON B.FUnitID=D.FItemID
WHERE A.FClosed!=1 AND B.FMrpClosed!=1 AND C.FNumber=@FNumber
UNION
--采购申请
SELECT '采购申请' [仓库/单据],'' 仓位,CAST(CAST((B.FQty-B.FCommitQty) AS DECIMAL(20,2)) AS NVARCHAR(20)) + D.FName 主计量,
CAST(CAST((B.FSecQty-B.FSecCommitQty) AS DECIMAL(20,2)) AS NVARCHAR(20)) 辅助计量,'' 批号 FROM dbo.PORequest A 
JOIN dbo.PORequestEntry B ON A.FInterID=B.FInterID
JOIN dbo.t_ICItem C ON B.FItemID=C.FItemID
JOIN dbo.t_MeasureUnit D ON B.FUnitID=D.FItemID
WHERE A.FClosed!=1 AND C.FNumber=@FNumber
END</value>
