<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout
        android:layout_weight="5"
        android:orientation="vertical"
        android:layout_width="0dp"
        android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
        <TextView
            android:id="@+id/activity_main_outstock_entry_layout_FNumber"
            android:text="FNumber"
            android:textSize="18dp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
                <View
                       android:layout_width="10dp"
                       android:layout_height="0dp"/>
            <TextView
                    android:id="@+id/activity_main_outstock_entry_layout_FName"
                    android:textSize="12dp"
                     android:textStyle="italic"
                android:text="FName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
                <View
                       android:layout_width="10dp"
                       android:layout_height="0dp"/>
            <TextView
                    android:id="@+id/activity_main_outstock_entry_layout_FModelr"
                    android:textSize="12dp"
                     android:textStyle="italic"
                android:text="FModel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
                </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">
                     <TextView
                android:id="@+id/activity_main_outstock_entry_layout_FStock"
                android:text="stock"
                android:fontFamily="sans-serif-smallcaps"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
                    <View
                       android:layout_width="10dp"
                       android:layout_height="0dp"/>
                    <TextView
                        android:id="@+id/activity_main_outstock_entry_layout_FStock_Name"
                         android:textSize="12dp"
                     android:textStyle="normal"
                         android:layout_weight="9"
                        android:gravity="left"
                        android:text="FName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    </LinearLayout>
                 <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">
                     <TextView
                android:id="@+id/activity_main_outstock_entry_layout_FStockPlace"
                android:fontFamily="sans-serif-smallcaps"
                android:text="stockplace"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
                      <View
                       android:layout_width="10dp"
                       android:layout_height="0dp"/>
                    <TextView
                        android:id="@+id/activity_main_outstock_entry_layout_FStockPlace_Name"
                         android:textSize="12dp"
                     android:textStyle="normal"
                         android:layout_weight="9"
                        android:gravity="left"
                        android:text="FName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    </LinearLayout>
           
           
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/activity_main_outstock_entry_layout_FQty"
                android:fontFamily="serif"
                android:text="FQty"
                android:textStyle="italic"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"/>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">
                     <TextView
                         android:id="@+id/activity_main_outstock_entry_layout_FMainUint_Name"
                         android:textSize="12dp"
                     android:textStyle="italic"
                         android:layout_weight="9"
                        android:gravity="left"
                        android:text="FName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
           
                     <View
                       android:layout_width="10dp"
                       android:layout_height="0dp"/>
                    <TextView
                       android:visibility="invisible"
                android:id="@+id/activity_main_outstock_entry_layout_FMainUint"
                android:fontFamily="serif"
                android:text="mainuint"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
                    </LinearLayout>

        </LinearLayout>
        <TextView
            android:id="@+id/activity_main_outstock_entry_layout_FNote"
            android:textStyle="italic"
            android:text="FNote"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>
<LinearLayout
        android:orientation="vertical"
        android:layout_weight="1"
    android:layout_width="0dp"
    android:layout_height="wrap_content">
        <Button
            android:id="@+id/activity_main_outstock_entry_layout_edit"
            android:text="@string/activity_main_instock_layout_Entry_edit_button"
            android:layout_margin="4dp"
            android:background="#dddddd"
            android:layout_width="match_parent"
            android:layout_height="20dp"/>
        <Button
            android:id="@+id/activity_main_outstock_entry_layout_delete"
            android:text="@string/activity_main_instock_layout_Entry_delete_button"
            android:layout_margin="4dp"
            android:background="#dddddd"
            android:layout_width="match_parent"
            android:layout_height="20dp"/>
        <Button
            android:id="@+id/activity_main_outstock_entry_layout_unknow"
            android:text="@string/activity_main_instock_layout_Entry_unknow_button"
            android:layout_margin="4dp"
            android:background="#dddddd"
            android:layout_width="match_parent"
            android:layout_height="20dp"/>
    </LinearLayout>
</LinearLayout>
        <View
            android:background="#000000"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>
</LinearLayout>
