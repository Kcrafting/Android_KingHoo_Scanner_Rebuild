<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:text="FNumber"
                android:id="@+id/activity_main_instock_entry_layout_SourceBill_FNumber"
                android:layout_weight="5"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_width="120dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:text="FName"
                android:id="@+id/activity_main_instock_entry_layout_SourceBill_FName"
                android:layout_weight="5"
                android:textSize="12dp"
                android:textStyle="italic"
                android:layout_width="120dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:text="FModel"
                android:layout_weight="5"
                android:id="@+id/activity_main_instock_entry_layout_SourceBill_FModel"
                android:textSize="12dp"
                android:textStyle="italic"
                android:layout_width="120dp"
                android:layout_height="wrap_content"/>
            </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/activity_main_instock_entry_layout_SourceBill_TotleQty"
                android:layout_weight="5"
                android:text="TotleQty"
                android:layout_width="120dp"
                android:fontFamily="sans-serif-smallcaps"
                android:layout_height="wrap_content"/>
            <TextView
                 android:id="@+id/activity_main_instock_entry_layout_SourceBill_CommitQty"
                android:layout_weight="5"
                android:text="FCommitQty"
                android:layout_width="120dp"
                android:textSize="12dp"
                     android:textStyle="normal"
                android:layout_height="wrap_content"/>
            <TextView
                 android:id="@+id/activity_main_instock_entry_layout_SourceBill_StockQty"
                android:layout_weight="5"
                android:text="StockQty"
                android:layout_width="120dp"
                android:textSize="12dp"
                     android:textStyle="normal"
                android:layout_height="wrap_content"/>
            <TextView
                 android:id="@+id/activity_main_instock_entry_layout_SourceBill_Unit"
                android:text="FUnit"
                android:layout_width="120dp"
                android:layout_weight="5"
                android:textSize="12dp"
                     android:textStyle="normal"
                android:layout_height="wrap_content"/>
            </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:text="选择仓库"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <EditText
                    android:id="@+id/activity_main_instock_entry_layout_SourceBill_Stock"
                    android:layout_weight="1"
                    android:layout_width="120dp"
                    android:textSize="12dp"
                    android:hint="选择仓库"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:text="仓位选择"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <EditText
                 android:id="@+id/activity_main_instock_entry_layout_SourceBill_StockPlace"
                 android:layout_weight="1"
                android:layout_width="120dp"
                android:textSize="12dp"
                android:hint="选择仓位"
                android:layout_height="wrap_content"/>

                </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:text="批号"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            <EditText
                 android:id="@+id/activity_main_instock_entry_layout_SourceBill_Batch"
                 android:layout_weight="1"
                android:layout_width="120dp"
                android:textSize="12dp"
                android:hint="输入批号"
                android:layout_height="wrap_content"/>
                <TextView
                    android:text="提交数量"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            <EditText
                android:id="@+id/activity_main_instock_entry_layout_SourceBill_Qty"
                 android:layout_weight="1"
                android:layout_width="120dp"
                android:textSize="12dp"
                android:hint="输入数量"
                android:layout_height="wrap_content"/>
                </LinearLayout>>
            </LinearLayout>
        <EditText
            android:id="@+id/activity_main_instock_entry_layout_SourceBill_Note"
            android:hint="输入备注"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
<!--        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <Button
                 android:id="@+id/activity_main_instock_entry_layout_SourceBill_Save"
                android:text="保存"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <Button
                 android:id="@+id/activity_main_instock_entry_layout_SourceBill_Edit"
                android:text="编辑"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <Button
                 android:id="@+id/activity_main_instock_entry_layout_SourceBill_Delete"
                android:text="删除"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            </LinearLayout>-->
        </LinearLayout>
</LinearLayout>
