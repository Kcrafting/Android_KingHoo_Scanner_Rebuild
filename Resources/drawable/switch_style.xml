<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
   <item android:state_checked="true">

            <shape>

                  <solid android:color="#33da33"/>  
                  <corners android:radius="15dp"/>  
                  <size android:width="30dp"  android:height="30dp"/>  
               <stroke android:color="#33da33" android:width="1dp" android:dashGap="0dp" android:dashWidth="0dp"/>  
    </shape>  
        
  </item>  
      <item android:state_checked="false">

            <shape>

                  <solid android:color="#aaaaaa" />  
                  <corners android:radius="15dp"/>  
                  <!--低版本手机需要设置size才能正常显示track-->  
                  <size android:width="30dp"  android:height="30dp" />  
                  <!--设置边框-->  
                  <stroke android:color="#aaaaaa" android:width="1dp" android:dashGap="0dp" android:dashWidth="0dp"/>  
              
    </shape>  
        
  </item>  

</selector>
